  <body>
    <%- include('../layout/navbar.ejs'); %>

    <div class="innerbox">

        <div>
            <h1 class="title">Natures</h1>
    
            <div class="content">
    
              <div class="maincontent">
                <a href="gitgit" target="_blank">
                  <img src="/image/ttd/naw/gigitwaterfall.jpg" alt="">
                  <div class="text">
                    <h3>1.Gitgit Waterfall</h3>                                
                  </div>
                </a>
                <div class="rating">
                  <img src="/image/ttd/Rating Star PNG/5star.png" alt="">
                </div> 
                <div class="text">
                  <p>Jl. Raya Bedugul - Singaraja, Gitgit, Kec. Sukasada, Kabupaten Buleleng, Bali 81161</p>
                </div>                 
              </div>
                             
              <div class="maincontent">
                <a href="baluran" target="_blank">
                  <img src="/image/ttd/naw/baluran.jpg" alt="">
                  <div class="text">
                    <h3>2.Baluran National Park</h3>                                        
